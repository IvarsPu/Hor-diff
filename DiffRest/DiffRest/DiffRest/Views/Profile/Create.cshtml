@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<input type="text" class="form-control" id="url" placeholder="URl" />
<br>
<input type="password" class="form-control" id="password" placeholder="Password" />
<br>
<input type="button" class="btn btn-primary mb-2" value="Register" onclick="register()" />

<script>
//register new profile
function register() {
    $.ajax({
        type: "GET",
        url: '/Profile/CreateProfile?url=' + $("#url").val() + '&password=' + $("#password").val(),
        success: function (i) {
            if (i != 0) {
                location.replace('/Process');
            }
            else {
                alert("Already exists");
            }
        },
        error: function () {
            alert('fail');
        }
    });
}
</script>